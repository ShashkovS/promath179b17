beginfig(5);

path p;
u := .4cm;

z1 = (0,5u);
z2 = (3u, 7u);
z3 = (6u, 4u);
z4 = (8u, 7u);
z5 = (9u, 2u);
z6 = (5u, 1u);
z7 = (2u, 2u);

pickup pencircle scaled 2.5pt;

p = z1{dir 90}..z2{dir -10}..z3{dir 20}..z4{dir -70}..z5{dir -130}..z6{dir 80}..z7..cycle;
draw p;

h = 11;

for i = 0 upto h:
	z[10+i] = point i*length(p)/h of p;
endfor;


pickup pencircle scaled .8pt;

draw z10
  for i = 10 upto h+10:
    --z[i]
endfor --cycle dashed evenly;






endfig;

