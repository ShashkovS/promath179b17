beginfig(2);

u := .7cm;

draw (3u,0)--(10u,0);

z0 = (5u,0);
z1 = (2u,2u);
z2 =  z1 rotatedaround( z0, -33);
z3 =  z1 rotatedaround( z0, -64);
z4 =  z1 rotatedaround( z0, -94);
z5 =  z1 rotatedaround( z0, -137);

z6 = (7u,1u);
z7 = (9u,3u);

draw 4/5[z0,z1] -- 9/6[z1,z0];
draw 4/5[z0,z2] -- 7/6[z2,z0];
draw 8/7[z0,z3] -- 8/7[z3,z0];
draw 7/5[z0,z4] -- 8/7[z4,z0];
draw 7/5[z0,z5] -- 9/7[z5,z0];

pickup pencircle scaled 3pt;

draw z6--z7;

label.rt(btex $A$ etex, z6);
label.urt(btex $B$ etex, z7);

for i = 1 upto 5:
   z[10*i+6] = whatever[z0,z[i]];
  (x[10*i+6]-x6)*(x[i]-x0) + (y[10*i+6]-y6)*(y[i]-y0) = 0;
   z[10*i+7] = whatever[z0,z[i]];
  (x[10*i+7]-x7)*(x[i]-x0) + (y[10*i+7]-y7)*(y[i]-y0) = 0;
  pickup pencircle scaled 1.5pt;
  draw z[10*i+6]--z[10*i+7];
  pickup pencircle scaled .5pt;
  draw z[10*i+6]--z6 dashed evenly;
  draw z[10*i+7]--z7 dashed evenly;
endfor;






endfig;

