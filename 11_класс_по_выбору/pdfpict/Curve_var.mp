z0 = (6u,5u);
z1 = (6u, 3u);
z2 = (3.5u,4u);
z3 = (4u, 2.5u);
z4 = (.5u,2.5u);
z5 = (8u,3u);

for i = 1 upto 5:
  x[10+i] = x[i]; y[10+i] = 0;
endfor;

